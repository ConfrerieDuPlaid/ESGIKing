  "bugs": {
    "url": "https://github.com/ConfrerieDuPlaid/ESGIKing/issues"
  },
  "homepage": "https://github.com/ConfrerieDuPlaid/ESGIKing#readme",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.23",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mongoose": "^6.2.9",
    "typedoc": "^0.22.15"
  },
  "types": "./dist/index.d.ts"
}
